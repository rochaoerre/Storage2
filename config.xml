<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.storage" version="1.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<preference name="phonegap-version" value="cli-9.0.0" />
  <name>Storage</name>
  <description>
      Esta es una App de PhoneGapp Files
  </description>
  <author email="djrochaoerre@hotmail.com" href="http://cordova.io">
      Ing. Luis Rocha
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
  <plugin name="cordova-plugin-dialogs"/> 
  <plugin name="cordova-plugin-device"/>
  <plugin name="cordova-plugin-media"/>
  <plugin name="cordova-plugin-file"/>
  <plugin name="cordova-plugin-file-transfer"/>
  <plugin name="cordova-plugin-sqlite-2"/>
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>
